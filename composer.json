{
  "name": "levmorozov/mii",
  "type": "library",
  "description": "Tiny php framework",
  "homepage": "https://github.com/levmorozov/mii",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "ext-mbstring": "*",
    "ext-mysqli": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "mii\\": "src/"
    },
    "classmap" : [
      "src/Mii.php"
    ],
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "miit\\": "tests/"
    }
  }
}
